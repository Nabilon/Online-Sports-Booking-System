<div>
    <label>FACILITY NAME</label>
    <input type="text" name="FacName" value="<%= facility.FacName %>">
</div>
<div>
    <label>FACILITY TYPE</label>
    <input type="text" name="FacType" value="<%= facility.FacType %>">
</div>
<div>
    <label>FACILITY NUMBER</label>
    <input type="number" name="FacNum" min="1" value="<%= facility.FacNum %>">
</div>
<div>
    <label>FACILITY IMAGE</label>
    <input type="file" name="cover" class="filepond">
</div>

